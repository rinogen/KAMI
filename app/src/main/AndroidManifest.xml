<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kami">

    <permission android:name="com.example.kami.READ_DATABASE" android:protectionLevel="normal"/>
    <permission android:name="com.example.kami.WRITE_DATABASE" android:protectionLevel="normal"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <provider
            android:name="com.example.kami.database.DatabaseProvider"
            android:authorities="com.example.kami"
            android:enabled="true"
            android:exported="true"
            android:readPermission="com.example.kami.READ_DATABASE"
            android:writePermission="com.example.kami.WRITE_DATABASE"/>

        <activity android:name=".TipsKesehatan"></activity>
        <activity android:name=".Riwayat" />
        <activity android:name=".AnalisisImt" />
        <activity android:name=".HasilKalkulasi" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>